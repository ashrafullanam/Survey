<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ashraf.survey">

    <application
        android:allowBackup="true"
        android:icon="@drawable/survey"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuardView" />
        <activity android:name=".QuardViewShow" />
        <activity android:name=".TriangleView" />
        <activity android:name=".TriangleViewShow" />
        <activity android:name=".TriangleView2" />
        <activity android:name=".TriangleViewShow2" />
        <activity android:name=".CircleView" />
        <activity android:name=".CircleViewShow" />
        <activity android:name=".FormulaView"></activity>
    </application>

</manifest>